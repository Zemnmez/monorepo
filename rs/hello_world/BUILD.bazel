load("//bzl:rules.bzl", "bazel_lint")
load("//rs:rules.bzl", "rust_binary", "rust_doc")

package(default_visibility = ["//visibility:public"])

rust_binary(
    name = "hello_world",
    srcs = ["src/main.rs"],
)

rust_doc(
    name = "hello_world_doc",
    crate = ":hello_world",
)
bazel_lint( name = "bazel_lint" )
