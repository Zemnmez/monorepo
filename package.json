	"scripts": {
		"build": "yarn bazel build //...",
		"test": "yarn bazel test //...",
		"fix-bazel": "yarn bazel run //:buildifier -- -- --lint=fix",
		"fix-go": "yarn run bazel run @go_sdk//:bin/gofmt -- -- -s -w .",
		"fix-js": "yarn run eslint --fix --ignore-path .gitignore '**/*.ts' '**/*.js' '**/*.tsx' '**/*.json'",
		"fix": "yarn run fix-go ; yarn run fix-js ; yarn run fix-bazel"
	},
	"dependencies": {
